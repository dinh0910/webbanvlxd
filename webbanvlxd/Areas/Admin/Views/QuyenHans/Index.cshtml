@model IEnumerable<webbanvlxd.Models.QuyenHan>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table" id="myTable">
    <thead>
        <tr>
            <th>
                Tên quyền hạn
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ten)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.QuyenHanID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.QuyenHanID">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
